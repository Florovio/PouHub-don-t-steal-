local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

getgenv().SecureMode = true

local Window = Rayfield:CreateWindow({
	Name = "PouHub",
	LoadingTitle = "PouHub",
	LoadingSubtitle = "The Next Generation",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
        Discord = {
        	Enabled = true,
        	Invite = "PouHub", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "PouHub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/PouHub)",
		FileName = "PouHub Key",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Pou"
	}
})

local Tab = Window:CreateTab("main", 4483362458) -- Title, Image

Rayfield:Notify({
    Title = "PouHub",
    Content = "PouHub opend succesful",
    Duration = 6.5,
    Image = 4483362458,
    Actions = { -- Notification Buttons
        Ignore = {
            Name = "Okay! idc",
            Callback = function()
                print("The user tapped Okay!")
            end
		},
	},
})

-- WalkSpeed --

local WalkSpeed_Slider = MainTab:CreateSlider({
	Name = "WalkSpeed",
	Range = {16, 150},
	Increment = 1,
	Suffix = "Speed",
	CurrentValue = 16,
	Flag = "Speed_Slider", 
	Callback = function(Value)
		WS = Value
	end
})

spawn(function() 
    while wait(1) do 
        pcall(function() 
            Humanoid.WalkSpeed = WS
        end)
    end
end)


local Label = Tab:CreateLabel("I reccomend 75-90 speed or it gonna be baneable")

-- inf Jump --
local JumpDebounce = false

local JumpCooldown = 0.1

local Button = MainTab:CreateButton({
	Name = "Inf Jumps",
	Callback = function()
		UIS.JumpRequest:Connect(function()
			if JumpDebounce == false then

				JumpDebounce = true

				Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
				wait(JumpCooldown)
				Humanoid:ChangeState(Enum.HumanoidStateType.Landed)

				JumpDebounce = false
			end
		end)
	end
})

-- Esp --

local Section = Tab:CreateSection("Esp")

local Toggle = Tab:CreateToggle({
	Name = "Player ESP",
	CurrentValue = false,
	Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(Value)
		local PlayerEsp_Toggler = MainTab:CreateToggle({
			Name = "Player Esp",
			CurrentValue = false,
			Flag = "PlayerESP",
			Callback = function(Value)
				if Value == true then
					for i,v in pairs(PlayerCharacters:GetChildren()) do
						if v:IsA("Model") then
							Char = v
		
							Shirt = Char:WaitForChild("Shirt")
							
							local HighlightClone = PlayerHighlight:Clone()
							HighlightClone.Adornee = Char
							HighlightClone.Parent = Char
							HighlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
						end
					end
					elseif  Value == false then
						for i,v in pairs(PlayerCharacters:GetChildren()) do
							if v:IsA("Model") then
								Char = v
			
								Shirt = Char:WaitForChild("Shirt")
								
								if Char:FindFirstChild("PlayerESP") then
									Char.PlayerESP:Destroy()
									else
										return
								end
							end
						end
				end
			end
		})
		
	end
})
})

